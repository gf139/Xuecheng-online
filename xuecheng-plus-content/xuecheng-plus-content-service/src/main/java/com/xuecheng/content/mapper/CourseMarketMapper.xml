<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuecheng.content.mapper.CourseMarketMapper">

    <insert id="insertCourseMarket" parameterType="com.xuecheng.model.po.CourseMarket">
        INSERT INTO course_market
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="charge != null">
                charge,
            </if>
            <if test="price != null">
                price,
            </if>
            <if test="originalPrice != null">
                original_price,
            </if>
            <if test="qq != null">
                qq,
            </if>
            <if test="wechat != null">
                wechat,
            </if>
            <if test="phone != null">
                phone,
            </if>
            <if test="validDays != null">
                valid_days,
            </if>
        </trim>
        <trim prefix="VALUES (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id},
            </if>
            <if test="charge != null">
                #{charge},
            </if>
            <if test="price != null">
                #{price},
            </if>
            <if test="originalPrice != null">
                #{originalPrice},
            </if>
            <if test="qq != null">
                #{qq},
            </if>
            <if test="wechat != null">
                #{wechat},
            </if>
            <if test="phone != null">
                #{phone},
            </if>
            <if test="validDays != null">
                #{validDays},
            </if>
        </trim>
    </insert>

</mapper>
